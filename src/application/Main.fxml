<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="500.0" style="-fx-background-color: white;" stylesheets="@application.css" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainController">
   <children>
      <Button id=".button" fx:id="filechooser" layoutX="175.0" layoutY="221.0" mnemonicParsing="false" onAction="#fileChooser" prefHeight="30.0" prefWidth="150.0" stylesheets="@application.css" text="File Chooser">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button id=".button" fx:id="login" layoutX="175.0" layoutY="334.0" mnemonicParsing="false" onAction="#Login" prefHeight="30.0" prefWidth="150.0" stylesheets="@application.css" text="Login">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button id=".button" fx:id="calculator" layoutX="175.0" layoutY="277.0" mnemonicParsing="false" onAction="#Calculator" prefHeight="30.0" prefWidth="150.0" stylesheets="@application.css" text="Calculator">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <ImageView fitHeight="80.0" fitWidth="119.0" layoutX="210.0" layoutY="395.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../Resources/img/3m-his-squarelogo.png" />
         </image></ImageView>
      <ImageView fitHeight="98.0" fitWidth="500.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../Resources/img/19594-his_social_media_wordpress_banner_990x180.jpg" />
         </image>
      </ImageView>
      <Button id=".button" fx:id="execute" layoutX="175.0" layoutY="163.0" mnemonicParsing="false" onAction="#Execute" prefHeight="40.0" prefWidth="150.0" stylesheets="@application.css" text="Execute / copy text" wrapText="true">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
